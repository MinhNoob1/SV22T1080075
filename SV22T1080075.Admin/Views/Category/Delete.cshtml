@{
    ViewBag.Title = "Xóa loại hàng";
}
<div class="box box-danger">
    <div class="box-body">
        <form action="#">
            <div class="form-group">
                <label>Tên loại hàng:</label>
                <p class="form-control-static">Điện gia dụng</p>
            </div>

            <div class="form-group">
                <label>Mô tả:</label>
                <p class="form-control-static">Tủ lạnh, nồi cơm điện, bếp từ,...</p>
            </div>

            <div class="form-group text-right">
                <button type="submit" class="btn btn-danger">
                    <i class="fa fa-trash"></i>
                    Xóa dữ liệu
                </button>
                <a href="~/Category" class="btn btn-default">
                    Quay lại
                </a>
            </div>

        </form>

    </div>
</div>

